<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<keys
   name="User Shortcuts">
  <bind
     gaction="app.copy"
     keys="&lt;Primary&gt;c,&lt;Primary&gt;Insert,y" />
  <bind
     gaction="app.object-align('hcenter pref')"
     keys="&lt;Primary&gt;backslash" />
  <bind
     gaction="app.object-align('vcenter pref')"
     keys="&lt;Primary&gt;minus" />
  <bind
     gaction="app.selection-group"
     keys="&lt;Primary&gt;g,&lt;Primary&gt;&lt;Shift&gt;u,g" />
  <bind
     gaction="app.selection-ungroup"
     keys="&lt;Primary&gt;&lt;Shift&gt;g,&lt;Primary&gt;u,&lt;Shift&gt;g" />
  <bind
     gaction="doc.redo"
     keys="&lt;Primary&gt;r" />
  <bind
     gaction="doc.undo"
     keys="u" />
  <bind
     gaction="win.paste"
     keys="&lt;Primary&gt;v,&lt;Shift&gt;Insert,p" />
</keys>
